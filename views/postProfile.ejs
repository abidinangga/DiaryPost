<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>My Post</title>
</head>
<body>
    <h1>My Post</h1>
    <table border="1">
        <tr>
            <th>Image</th>
            <th>Title</th>
            <th>Tag</th>
            <th>Action</th>
        </tr>
        <% posts.forEach(el => { %>
            <tr>
                <td><img src="<%= el.imageUrl %>" style="width:200px"></td>
                <td><%= el.title %> </td>
                <td><%= el.Tag.name %> </td>
                <td>
                    <button onclick="location.href='/profile/<%= el.UserId %>/post/detail'">Detail</button>
                </td>
            </tr>
        <% }) %> 
    </table>
</body>
</html>